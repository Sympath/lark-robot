{
  "name": "feishu-webhook-server",
  "version": "1.0.9",
  "description": "Feishu Webhook Server with TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "postbuild": "echo 'Build completed successfully'",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "start:dev": "NODE_ENV=development ts-node src/index.ts",
    "docker:build": "docker build -t feishu-webhook .",
    "docker:run": "docker run -p 3000:3000 feishu-webhook",
    "nginx:start": "sudo nginx -c $(pwd)/nginx.conf",
    "nginx:stop": "sudo nginx -s stop",
    "nginx:reload": "sudo nginx -s reload",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "feishu",
    "webhook",
    "typescript",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@koa/router": "^14.0.0",
    "@larksuiteoapi/node-sdk": "^1.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "koa": "^2.14.2",
    "koa-bodyparser": "^4.4.1",
    "koa-cors": "^0.0.16",
    "koa-logger": "^3.2.1",
    "koa-router": "^12.0.0",
    "koa-static": "^5.0.0",
    "morgan": "^1.10.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/koa": "^2.13.11",
    "@types/koa__cors": "^5.0.0",
    "@types/koa__router": "^12.0.4",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa-cors": "^0.0.2",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.4.4",
    "@types/koa-static": "^4.0.2",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
